page{
   font-family: "黑体";
   box-sizing: border-box;
   padding-bottom: 100rpx;
} 
.uybg{
	position: absolute;
	left: 0;
	top: 0;
	z-index: -1;
	width: 100%;
	height: 100%;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
}
.top-banner{
  position: relative;
    padding:40rpx 0 0;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
}
.uycenter_head2{
    width:100%;
    overflow: hidden;
    padding:0 12% ;
    position: relative;
    display: flex;
} 

.uylmenu{
  flex: 1;
  margin-left: 5%;
  overflow:hidden;
}
.uylmenu1{
    width:100%;
    overflow: hidden;
}
.uylmenu11{
    width: 100%;
    height:50rpx;
    line-height:50rpx;
    font-size:var(--fontSize);
    color: #fff;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.uylmenu11 text{
  margin-left: 8%;
  display: inline-block;
  padding:0 15rpx;
  font-size: 26rpx;
  line-height: 40rpx;
  margin-top:5rpx;
  background: #ec3839;
  box-shadow:  0 0 15rpx #d35584;
  border-radius: 20rpx;
}
.uylmenu12{
    width:100%;
    height:60rpx;
    line-height:60rpx;
    font-size:var(--fontSize);
    color: #fff;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.uylmenu12 text{
  font-size: 26rpx;
}

.head{
  flex: 0 0 130rpx;
  width: 130rpx;
  height: 130rpx;
  border:2px solid rgba(255, 255, 255, 1);
  border-radius: 50%;
}


.tabs{
  padding:20rpx 12% 0;
  background: #fff;
  font-size: 0;
}
.item{
  display: inline-block;
  width:50%;
  height:70rpx;
  line-height:70rpx;
  font-size:var(--fontSize);
  color: #ec3839;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-bottom:20rpx;
}
.label{
  margin-right: 10rpx;
  display: inline-block;
  padding:0 10rpx;
  font-size: 26rpx;
  color: #fff;
  line-height: 40rpx;
  background: #ff62a2;
  box-shadow:  0 0 15rpx #d35584;
  border-radius: 10rpx;
}
.yuan{
  font-size: 26rpx;
  color: #aaa;
  margin-left: 10rpx;
}
.black-font{
  color: #4c4c4c!important;
}
.text-right{
  text-align: right;
}





.swiper-tab {
  width: 100%;
  box-shadow: 0px -5px 15px #eee;
  overflow: hidden;
  background: #fff;
}

.swiper-tab-list {
  width: 50%;
  float: left;
  color: #4c4c4c;
  font-size:var(--fontSize);
  text-align: center;
  line-height:100rpx;
  border-bottom: 1px solid #f8f8f8;
}

.on {
  color: #ff6ea9;
  border-bottom: 1px solid #ff6ea9;
}

.list {
  width: 100%;
  font-size: 28rpx;
  background: #fff;
}

.top {
  width:100%;
  overflow: hidden;
   margin-top:20rpx; 
  color: #fff;
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
}

.top view {
  width: 33.3%;
  float: left;
  text-align: center;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-overflow: -o-ellipsis-lastline;
}

.list .te view {
  width: 20%;
  float: left;
  text-align: center;
}

.list .top view:last-child {
  width: 40%;
  float: left;
}

.list .bn {
  width: 100%;
  overflow: hidden;
  color: #4c4c4c;
  background: #fff;
  font-size: 26rpx;
  display: table;
}

.list .bn .sub_list {
  width:100%;
  overflow: hidden;
  padding:10rpx 0;
}

.list .bn .sub_list view {
  width: 27.3%;
  padding:0 3%;
  float: left;
  text-align: center;
  font-size: 26rpx;
  line-height: 40rpx;
  word-break: break-all;
  color: #999; 
  display: table-cell;
  vertical-align: middle;
}
.odd{
  background: #f3f3f3;
}
.red-color {
  color: #ff3333!important;
}
.black-color{
  color: #4c4c4c!important;
}


.img-head{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  left: 50%;
  margin-left: -75rpx;
  border:1px solid #fff;
  border-radius: 50%;
  top:-75rpx;
}
.img-close{
  position: absolute;
  right: 3%;
  top:2%;
  width: 50rpx;
  height: 50rpx;
}
.tit-info{
  width: 100%;
  background: var(--pinkcolor);
  padding-top:80rpx;
  border-top-left-radius: 9px;
  border-top-right-radius: 9px;
  padding-bottom: 15rpx;
}
.tit-name{
  font-size: 26rpx;
  color: #fff;
  text-align: center;
  line-height: 45rpx;
}
.box-fu{
  width: 94%;
  margin:20rpx auto;
  border-bottom: 1px solid #f8f8f8;
}
.box-info{
  font-size: 26rpx;
  color: #656565;
  text-align: center;
  line-height: 50rpx;
}
.box-money{
  font-size: 30rpx;
  color: var(--pinkcolor);
  font-weight: bold;
  text-align: center;
  line-height: 70rpx;
}



.fu-leftright {
  width: 94%;
  padding:0 3%;
  margin: 0 auto;
  height: 120rpx;
  overflow: hidden;
}

.fu-left {
  width: 27%;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
  color: #4c4c4c;
  overflow: hidden;
  float: left;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.fu-right {
  max-width: 73%;
  min-width: 73%;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
  color: #4c4c4c;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
}
.gray-backgound{
  background: #f0f0f0;
  border-bottom-left-radius: 9px;
  border-bottom-right-radius: 9px;
}
.fu-info{
  width: 94%;
  margin:0 auto;
  font-size: 20rpx;
  color: #aaa;
  text-align: center;
  line-height: 40rpx;
}
.fu-btn{
  width:30%;
  margin:70rpx auto 20rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  letter-spacing: 5rpx;
  color: #fff;
  overflow: hidden;
  text-align: center;
   border-radius: 20px;
   background: var(--pinkcolor);
}
.box-withdraw{
  font-size: 26rpx;
  color: #777;
  text-align: center;
  line-height: 70rpx;
  text-decoration: underline;
}
