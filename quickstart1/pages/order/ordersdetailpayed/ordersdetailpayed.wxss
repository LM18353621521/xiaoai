page{
  background: #f2f2f2;
  padding-bottom: 150rpx;
}
.top-banner{
  padding:20rpx 10%;
  width: 80%;
  background: #ff6aa4;
  position: relative;
}
.top-bannera{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: var(--bigFont);
  color: #fff;
  overflow: hidden;
}
.top-bannera text{
  margin-left: 7rpx;
  font-size: 24rpx;
}
.top-bannerb{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size:26rpx;
  color: #fff;
  overflow: hidden;
}
.payed{
  position: absolute;
  width: 120rpx;
  height: 120rpx;
  right:10%;
  top:10rpx;
}



.order-addr{
  background: #fff;
  overflow: hidden;
  position: relative;
  width: 80%;
  margin:0rpx auto;
  padding:20rpx 10%;
  display: flex;
}
.addr{
  width: 31rpx;
  height: 39rpx;
  flex: 0 0 31rpx;
  margin-top:36rpx;
}
.toright{
  position:absolute;
  right:6%;
  top:38rpx;
  width:19rpx;
  height:31rpx;
}
.addr-box{
  flex: 1;
  margin-left: 15rpx;
  overflow: hidden;
}
.addr-name{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color:var(--color);
  overflow: hidden;
}
.addr-name text{
  float: right;
  font-size: 26rpx;
}

.addr-info{
  width: 100%;
  line-height: 40rpx;
  font-size:24rpx;
  color:#707070;
  overflow: hidden;
}





.lists{
	width: 100%;
  background: #fff;
}
.item{
	width: 80%;
	padding: 20rpx 10%;
	margin:0 auto 10rpx;
	background: #eeeeee;
	overflow: hidden;
  display: flex;
  border-bottom: 1px solid #f8f8f8;
}
.item:last-child{
	margin:0 auto 0rpx;
}
.lt{
	width:172rpx;
  flex: 0 0 172rpx;
  height: 172rpx;
  margin-right: 4%;
}
.rt{
  flex: 1;
}
.lt image{
	width: 100%;
	height: 100%;
	border-radius: 2px;
}
.rt view.list-name{
  width: 100%;
	font-size: var(--fontSize);
  height: 80rpx;
	line-height:40rpx;
	color: #4c4c4c;
	overflow:hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 10rpx;
}
.rt view.list-info{
	font-size: 26rpx;
	color: #aaa;
  line-height: 40rpx;
	overflow:hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.rt view.list-info text{
  margin-right: 30rpx;
}
.rt view.list-money{
	font-size:26rpx;
  height: 60rpx;
	line-height:60rpx;
	color: #444444;
	overflow:hidden;
    text-overflow:ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.aa{
	font-size: var(--fontSize);
	color: #ff4a00;
}
.aa text{

}
.bb{
	float: right;
	width: 28%;
	text-align: right;
	font-size: var(--fontSize);
	color: #4c4c4c;
}


.view-input{
  width: 80%;
  padding:0 10%;
  overflow: hidden;
  border-bottom: 1px solid #f2f2f2;
  background: #fff;
}
.view-input-left{
  width:25%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: var(--fontSize);
  color: #4c4c4c;
  overflow: hidden;
  float: left;
  white-space: nowrap;
  text-overflow: ellipsis;
}
