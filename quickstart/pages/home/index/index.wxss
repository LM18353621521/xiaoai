page {
  font-family: "黑体";
  box-sizing: border-box;
  background: #e8e8e8;
  padding-bottom: 10rpx;
}

.scroll_box {
  width: 100%;
  /* height: 90rpx; */
  overflow: hidden;
  /* padding: 20rpx; */
  background: #fff;
  white-space: nowrap;
}

.item_list {
  width: 20%;
  height: auto;
  /* margin-right: 23rpx; */
  display: inline-block;
}

.item_list image {
  width: 90rpx;
  height: 90rpx;
  display: block;
  margin: 40rpx auto 0;
}

swiper {
  width: 100%;
  /* min-height: 450rpx; */
}

swiper image {
  width: 100%;
}

.searchbox {
  width: 80%;
  background: rgba(255, 255, 255, 1);
  position: absolute;
  left: 10%;
  top: 14rpx;
  border-radius: 25px;
  z-index: 9999;
}

.search {
  min-width: 80%;
  max-width: 80%;
  height: 54rpx;
  display: block;
  padding: 0 8%;
  overflow: hidden;
  font-size: var(--fontSize);
  color: #6c6c6c;
  line-height: 54rpx;
  background: #f2f3f5;
  border-radius: 30px;
  background: none;
}

.searchimg {
  position: absolute;
  left: 3%;
  top: 15rpx;
  width: 28rpx;
  height: 28rpx;
  z-index: 999;
}

.box {
  overflow: hidden;
  padding-bottom: 16rpx;
  background: #fff;
}

.viewinfo {
  width: 20%;
  overflow: hidden;
  float: left;
}

.viewinfo image {
  width: 90rpx;
  height: 90rpx;
  display: block;
  margin: 40rpx auto 0;
}

.viewinfotit {
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  font-size: var(--fontSize);
  overflow: hidden;
  color: #4c4c4c;
  text-align: center;
}

.product-list-box {
  width: 650rpx;
  padding: 0 50rpx;
  overflow: hidden;
  background: #e8e8e8;
  padding-top: 10px;
}

.product-list-title {
  width: 100%;
  height: 83rpx;
  line-height: 83rpx;
  color: #4c4c4c;
  font-size: 40rpx;
  overflow: hidden;
  text-align: center;
  margin-top: 10px;
  background: #fff;
}

.roduct_list_box100 {
  overflow: hidden;
  width: 100%;
  margin: 0 auto;
}

.iste1 {
  width: 305rpx;
  float: left;
  background: white;
  overflow: hidden;
  box-sizing: border-box;
  border: 1px solid #f2f3f5;
  border-radius: 7px;
  margin-bottom: 30rpx;
}

.iste1:nth-child(odd) {
  margin-right: 40rpx;
}

.eft11 {
  display: block;
  width: 100%;
  margin: 0 auto;
  height: 300rpx;
}

.ight1w {
  padding: 0 12px;
  width: 281rpx;
  overflow: hidden;
}

.ight12w {
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  font-size: var(--fontSize);
  color: #4c4c4c;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.ightw {
  width: 100%;
  overflow: hidden;
  margin-bottom: 10rpx;
}

.ightw1 {
  width: 100%;
  font-size: var(--fontSize);
  color: #ff4a00;
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.ightw1 text {
  font-size: 20rpx;
  color: #959595;
  margin-left: 5px;
}

.coupon {
  position: fixed;
  left: 4%;
  bottom: 5%;
  z-index: 9999;
}

.a1 {
  overflow: hidden;
  width: 244rpx;
  height: 89rpx;
  position: relative;
  margin-bottom: 20rpx;
}

.coupon-bg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.a11 {
  width: 60%;
  float: left;
  overflow: hidden;
}

.a01 {
  width: 100%;
  overflow: hidden;
  height: 45rpx;
  line-height: 45rpx;
  font-size: var(--fontSize);
  color: #ee268a;
  text-align: center;
}

.a02 {
  width: 100%;
  overflow: hidden;
  height: 20rpx;
  line-height: 20rpx;
  font-size: 12rpx;
  color: #3e3e3e;
  text-align: center;
}

.a12 {
  width: 40%;
  float: left;
  height: 89rpx;
  line-height: 89rpx;
  font-size: 20rpx;
  color: #fff;
  overflow: hidden;
  text-align: center;
}

.fuceng {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99999;
}

.fuer {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1200;
}

.fucengbox {
  width: 80%;
  position: fixed;
  left: 10%;
  top: 40%;
  z-index: 2000;
  background: #fff;
  border-radius: 3rpx;
}

.fucengtit {
  width: 88%;
  margin: 0 auto;
  height: 100rpx;
  line-height: 100rpx;
  color: #363636;
  font-size: 40rpx;
  overflow: hidden;
}

.leftrightf {
  width: 88%;
  margin: 0 auto;
  overflow: hidden;
  display: flex;
}

.leftf {
  flex: 0 0 84rpx;
  width: 84rpx;
  height: 84rpx;
  float: left;
  margin-right: 5%;
}

.rightf {
  flex: 1;
  line-height: 42rpx;
  font-size: var(--fontSize);
  letter-spacing: 2rpx;
  color: #363636;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.span {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  width: 88%;
  margin: 0 auto;
  font-size: var(--fontSize);
  color: #aaa;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid #f8f8f8;
}

.span text {
  background: #aaa;
  width: 14rpx;
  height: 14rpx;
  vertical-align: middle;
  display: inline-block;
  border-radius: 50%;
  margin-right: 3%;
  overflow: hidden;
}

.fucengbtn {
  overflow: hidden;
  width: 88%;
  margin: 0 auto;
  text-align: right;
}

.fucengbtn1 {
  width: 20%;
  height: 100rpx;
  line-height: 100rpx;
  color: #818181;
  font-family: "黑体";
  font-size: var(--fontSize);
  overflow: hidden;
  text-align: center;
  display: inline-block;
}

.fucengbtn2 {
  width: 20%;
  height: 100rpx;
  line-height: 100rpx;
  color: #279726;
  font-family: "黑体";
  font-size: var(--fontSize);
  overflow: hidden;
  text-align: center;
  display: inline-block;
}
