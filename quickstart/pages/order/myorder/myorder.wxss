  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid #f8f8f8; 
}
.span text{
  background: #aaaaaa;
  width: 14rpx;
  height: 14rpx;
  vertical-align: middle;
  display: inline-block;
  border-radius: 50%;
  margin-right: 3%;
	overflow: hidden;
}
.fucengbtn{
   overflow: hidden;
   width:88%;
   margin:0 auto;
   text-align: right;
}
.fucengbtn1{
   width:20%;
   height: 100rpx;
   line-height: 100rpx;
   color: #818181;
   font-family: "黑体";
   font-size: var(--fontSize);
   overflow: hidden;
   text-align: center; 
   display: inline-block;
}
.fucengbtn2{
   width:20%;
   height: 100rpx;
   line-height: 100rpx;
   color: #279726;
   font-family: "黑体";
   font-size: var(--fontSize);
   overflow: hidden;
   text-align: center; 
   display: inline-block;
}




.baibga{
   width:90%;
   position: fixed;
   left:5%;
   top: 15%;
   background: #fff;
   max-height:60%;
   overflow: hidden;
   z-index: 20000;
   border-radius: 5px;
}
.login-title{
  background: #ff6ea9;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: var(--fontSize);
  color: #fff;
  position: relative;
}
.login-title image{
  position: absolute;
  right: 3%;
