button {
  position:relative;
  display:block;
  margin-left:auto;
  margin-right:auto;
  padding-left:0px;
  padding-right:0px;
  box-sizing:border-box;
  font-size:18px;
  text-align:center;
  text-decoration:none;
  line-height:2.55555556;
  border-radius:0px;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  color:#000000;
  /* background-color:#fff!important; */
  background: #fff;
  border:none;

}
button::after {
  background:none;
  border:none;
  border-right: 1px solid #f8f8f8;
}

