/*字体基本样式  */
.base{
  width: 100%;
  margin:0 auto;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #fff;
  overflow: hidden;
  text-align: center;
  font-weight: bold;
   border-radius: 30px;
   background: #FFC1B5;
   border:1px solid #ddd;
}
.base text{
  font-size:50rpx;
  font-weight: bold;
}
/*定位  */
.pos1{
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  z-index: 99999;
}
.pos2{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  z-index: 99999;
}
/*图片定位  */
.circle image{
  position: absolute;
  left: 0;
  top:0;
  z-index: -1;
  width: 100%;
  height: 140rpx;
}

.img-auto{
   width: 76%;
   display: block;
   margin:50rpx auto;
}
/*边框等  */
.baseborder{
   border:1px solid #f8f8f8;
   border-radius: 20px;
   
   box-shadow: 0 0 15rpx #ddd;
   box-sizing: border-box;
}

/*左右带图片样式  */
