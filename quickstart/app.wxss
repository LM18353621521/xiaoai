/*时间底部滚动器*/
.addr_layer .chose{
    border-top: 1px solid #ff6600;
    border-bottom: 1px solid #ff6600;
    color: #4d4d4d;
}
.addr_layer picker-view-column{
    height: 400rpx;
    text-align: center;
}
.addr_layer picker-view-column view{
    height: 72rpx;
    line-height: 72rpx;
}
.addr_layer{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5); 
    z-index: 10000;
}
.addr_layer .chose{color: #ff6600;}
.addr_layer .btns{
    display: flex;
    justify-content: space-between;
    /*align-items: center;*/
    width: 94%;
    padding: 0 3%;
    color: #ff6600;
    font-size: 30rpx;
    background: #f2f2f2;
}
.addr_layer .btns view{
    height: 96rpx;
    line-height: 96rpx;
    padding: 0 10rpx;
}
.addr_layer>view{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
    z-index: 10000;
}





/*地址选择*/
.citypickers{
  position: fixed;
  height: 100%;
  width: 100%;
  min-height: 100%;
  background-color: red;
}
.citypicker{
    position: fixed;
  bottom: 0px;
  width: 100%;
  background: #fff;
  z-index: 9999;
}
.cityheader {
  position: absolute;
  top:0px;
  width: 100%;
  z-index: 4;
  font-size: 28rpx;
  background: #FBF9FE;
}
.city-cancel {
  float: left;
  margin: 20rpx;
  color: #586C94;
}
.city-true {
  float: right;
  margin: 20rpx;
  color: #586C94
}
.section .picker {
  background-color: #fff;
  border-bottom: 1px #d9d9d9 solid;
  border-top: 1px #d9d9d9 solid;
  padding: 20rpx;
}
picker-view-column{
  font-size: 28rpx;
  text-align: center;
}
picker-view-column:nth-child(4){
  font-size: 28rpx;
  text-align: center;
  /* display: none; */
}



